{
  "name": "ecg-analyzer-mobile",
  "version": "1.0.0",
  "description": "AI-powered ECG diagnosis mobile app with real-time backend integration",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "keywords": [
    "ecg",
    "diagnosis",
    "ai",
    "medical",
    "react-native",
    "expo",
    "healthcare"
  ],
  "author": {
    "name": "ECG Diagnosis Team",
    "contributors": [
      "Elissa Matlock",
      "Eugene Ho",
      "Jonah Zembower"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "expo": "^54.0.30",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-router": "~6.0.21",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets-core": "^1.6.2",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|expo|@expo|@unimodules|unimodules)/)"
    ]
  },
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
